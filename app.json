{
  "expo": {
    "name": "My Rec",
    "slug": "My Rec",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-dgf.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app-icon-dgf.png",
      "resizeMode": "contain",
      "backgroundColor": "#121417"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myrecovery.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-dgf.png",
        "backgroundColor": "#121417"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.myrecovery.app"
    },
    "web": {
      "favicon": "./assets/images/app-icon-dgf.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "My Rec",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://8b7nmybw2syrfb2v2uutkkpz5cyh8a78.app.specular.dev"
    }
  }
}